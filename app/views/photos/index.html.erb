<h1>photos</h1>
<ul>
    <% @photos.each do |photo| %>
        <li>
            <%= image_tag(photo.uploaded_image.service_url, style:'width: 300px') %>
            <%= photo.title %>
            <%= photo.price %> 
            <%= photo.description %>
            <%= link_to "show", photo_path(photo.id) %>
        </li>
    <% end %>
</ul>