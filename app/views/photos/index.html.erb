<h1>photos</h1>
<ul>
    <% @photos.each do |photo| %>
        <li>
            <%= image_tag photo.uploaded_image, style:'width: 300px' if photo.uploaded_image.attached? %>
            <%= photo.title %>
            <%= photo.price %> 
            <%= photo.description %>
            <%= link_to "show", photo_path(photo.id) %>
        </li>
    <% end %>
</ul>